        <div class="col-sm-3 col-xs-12 itens left">
            <div class="fish"></div>
        </div>

        <div class="col-sm-3 col-xs-12 itens left">
            <div class="app-design3">
                <div class="camada">
                    <h3>Phone Mockup</h3>
                    <span class="circulo plus"></span>
                </div>
            </div>            
        </div>

