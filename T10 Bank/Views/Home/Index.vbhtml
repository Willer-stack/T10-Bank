<div class="contato col-sm-12 col-xs-12" id="contato">
    <div class="col-sm-12 col-xs-12 contato-1 up">
        <p>Contato</p>
        <h3>Envie sua mensagem</h3>
    </div>

    <div class="col-sm-12 col-xs-12 contato-2 up">
        <form>
            <div class="col-sm-12 col-xs-12 formulario">
                <input type="text" name="Nome" placeholder="Nome" />
            </div>

            <div class="col-sm-6 col-xs-12 formulario">
                <input type="text" name="Telefone" placeholder="Telefone" id="telefone"/>
            </div>

            <div class="col-sm-6 col-xs-12 formulario">
                <input type="text" name="E-mail" placeholder="E-mail" />
            </div>

            <div class="col-sm-12 col-xs-12 formulario">
                <textarea placeholder="Mensagem" name="Mensagem"></textarea>
            </div>
        </form>
    </div>

    <div class="col-sm-12 col-xs-12 contato-3 up">
        <a class="btn btn-default" href="#">Enviar</a>
    </div>

    <div class="back left"></div>

</div>
